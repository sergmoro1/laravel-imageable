{
    "name": "sergmoro1/laravel-imageable",
    "description": "The ability to upload multiple images to any model",
    "type": "library",
    "authors": [
        {
            "name": "Sergey Morozov",
            "email": "sergmoro1@ya.ru"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.0",
        "imagine/imagine": "~0.5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.6"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sergmoro1\\Imageable\\": "src/",
            "Sergmoro1\\Imageable\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sergmoro1\\Imageable\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
		"test": [
	  		"vendor/bin/phpunit"
		]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sergmoro1\\Imageable\\ImageableServiceProvider"
            ]
        }
    }
}
